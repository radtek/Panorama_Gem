<div class="page_caption" style="margin-bottom: 10px;"><%= t(:dba_schema_show_object_nach_file_caption, :default=>'Find object name for single block') %></div>

<% @update_area = get_unique_area_id %>
<%= my_ajax_form_tag url_for(:controller=>:dba_schema, :action=>:list_object_nach_file_und_block, :update_area=>@update_area) do %>
  File-Number
  <%= text_field_tag(:fileno, "", :size=>40) %>
  Block-Number
  <%= text_field_tag(:blockno, "", :size=>40) %>
  <%= submit_tag(t(:dba_schema_show_object_nach_file_submit_caption, :default=>'Find name of object'), :title => t(:dba_schema_show_object_nach_file_submit_hint, :default=>'Find name of object by block in DB-cache or by DBA_Extents')) %>
<% end %>

<div id="<%= @update_area %>" >
</div>
